                        <button class="btn btn-outline-secondary change" id="_group15-change" name="change" type="button" (click)="!eventProcessing && change()" [disabled]="eventProcessing || isVisibilityInactive('_group15','change')">
                          <span>{{ 'awag.prj.uc.contactmaster.sc.contactmaster.fl._group15.change' | translate }}</span>
                        </button>
                      </div>
                    </div>
                  }
                  </div>
                  <!-- 削除: BUTTON -->
                  <div class="col-md-1 col-sm-6 col-12 pb-2 col-ext col-delete">
                  @if (!isVisibilityHidden('_group15','delete')) {
                    <div class="" [awagVisibility]="visibilityConfig['_group15']['delete']">
                      <div class="input-group">
                        <button class="btn btn-outline-secondary delete" id="_group15-delete" name="delete" type="button" (click)="!eventProcessing && delete()" [disabled]="eventProcessing || isVisibilityInactive('_group15','delete')">
                          <span>{{ 'awag.prj.uc.contactmaster.sc.contactmaster.fl._group15.delete' | translate }}</span>
                        </button>
                      </div>
                    </div>
                  }
                  </div>
                  <!-- 閉じる: BUTTON -->
                  <div class="col-md-1 col-sm-6 col-12 pb-2 col-ext col-close">
                  @if (!isVisibilityHidden('_group15','close')) {
                    <div class="" [awagVisibility]="visibilityConfig['_group15']['close']">
                      <div class="input-group">
                        <button class="btn btn-outline-secondary close" id="_group15-close" name="close" type="button" (click)="!eventProcessing && close()" [disabled]="eventProcessing || isVisibilityInactive('_group15','close')">
                          <span>{{ 'awag.prj.uc.contactmaster.sc.contactmaster.fl._group15.close' | translate }}</span>
                        </button>
                      </div>
                    </div>
                  }
                  </div>
                </div>
              </div>
              <div class="d-grid gap-2 d-md-flex justify-content-md-center registration-button-group">
              </div>
            </ng-template>
          </div>
        </div>
      </div>
    </div>
  </div>
}
</form>
<div class="awag-footer-spacer"></div>
