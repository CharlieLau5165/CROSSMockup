<!-- Generated by AWAG -->
<router-outlet name="modal"></router-outlet>
<h1 class="fs-4 pb-2">{{ 'awag.prj.uc.maintenance.sc.land.name' | translate}}</h1>
<form name="form" #form="ngForm" class="awag-form-maintenance-land">
  <app-awag-alert></app-awag-alert>
@if (!isVisibilityHidden('_group0')) {
  <div class="awag-section-_group0 mb-3" [awagVisibility]="visibilityConfig['_group0']['*']">
    <div ngbAccordion>
      <div ngbAccordionItem [collapsed]="false" [disabled]="false">
        <h2 ngbAccordionHeader>
          <button class="" id="_group0-accordion" name="_group0-accordion" type="button" ngbAccordionButton>{{ 'awag.prj.uc.maintenance.sc.land.fl._group0._name' | translate }}</button>
        </h2>
        <div ngbAccordionCollapse>
          <div ngbAccordionBody>
            <ng-template>
              <div class="container-fluid">
                <div class="row">
                  <!-- 運輸局: SELECT -->
                  <div class="col-md-3 col-sm-6 col-12 pb-2 col-ext col-transportdept">
                  @if (!isVisibilityHidden('_group0','transportdept')) {
                    <div class="" [awagVisibility]="visibilityConfig['_group0']['transportdept']">
                      <div class="has-validation">
                        <label class="form-label" for="_group0-transportdept">{{ 'awag.prj.uc.maintenance.sc.land.fl._group0.transportdept' | translate }}</label>
                        <div class="input-group awag-validity">
                          <select class="form-select awag-validity transportdept" id="_group0-transportdept" name="transportdept" [(ngModel)]="model.search.transportdept" #_group0_transportdept="ngModel" awagSelect (change)="transportdeptOnChange()">
                            <option value=""></option>
                            <option *ngFor="let opt of transportdeptOptsCode" [value]="opt.value">{{opt.name}}</option>
                          </select>
                        </div>
                        <div class="invalid-feedback transportdept-feedback" id="_group0-transportdept-feedback">{{ 'awag.label.sc.select.required' | translate }}</div>
                      </div>
                    </div>
                  }
                  </div>
                </div>
              </div>
              <div class="d-grid gap-2 d-md-flex justify-content-md-center">
              </div>
            </ng-template>
          </div>
        </div>
      </div>
    </div>
  </div>
}
@if (!isVisibilityHidden('list')) {
  <div class="awag-section-list mb-3" [awagVisibility]="visibilityConfig['list']['*']">
    <div ngbAccordion>
      <div ngbAccordionItem [collapsed]="false" [disabled]="false">
        <h2 ngbAccordionHeader>
          <button class="" id="list-accordion" name="list-accordion" type="button" ngbAccordionButton>{{ 'awag.prj.uc.maintenance.sc.land.fl.list._name' | translate }}</button>
        </h2>
        <div ngbAccordionCollapse>
          <div ngbAccordionBody>
            <ng-template>
              <div class="table-responsive mb-2 awag-table">
                <table class="table table-responsive table-striped table-hover" attr.aria-label="{{ 'awag.prj.uc.maintenance.sc.land.fl.list._name' | translate }}">
                  <thead>
                    <tr>
                      <!-- 選択: BUTTON -->
                      <th colspan="1" scope="col" aria-sort="none"></th>
                      <!-- 支局等: OUTPUT -->
                      <th colspan="1" scope="col" aria-sort="none">
                      @if (!isVisibilityHidden('list','branch')) {
                        <div [awagVisibility]="visibilityConfig['list']['branch']">
                          <a class="link-body-emphasis link-underline link-underline-opacity-0 link-opacity-50-hover awagTableHeader-branch" id="list-awagTableHeader-branch" href="javascript:void(0)" [awagSortTable]="listListCtrl" sortKey="branch">
                            <span>{{ 'awag.prj.uc.maintenance.sc.land.fl.list.branch' | translate }}</span>
                            <i class="bi bi-arrow-up"></i><i class="bi bi-arrow-down"></i>
                          </a>
                        </div>
                      }
                      </th>
                      <!-- 標版文字: OUTPUT -->
                      <th colspan="1" scope="col" aria-sort="none">
                      @if (!isVisibilityHidden('list','name')) {
                        <div [awagVisibility]="visibilityConfig['list']['name']">
                          <a class="link-body-emphasis link-underline link-underline-opacity-0 link-opacity-50-hover awagTableHeader-name" id="list-awagTableHeader-name" href="javascript:void(0)" [awagSortTable]="listListCtrl" sortKey="name">
                            <span>{{ 'awag.prj.uc.maintenance.sc.land.fl.list.name' | translate }}</span>
                            <i class="bi bi-arrow-up"></i><i class="bi bi-arrow-down"></i>
                          </a>
                        </div>
                      }
                      </th>
                      <!-- コード: OUTPUT -->
                      <th colspan="1" scope="col" aria-sort="none">
                      @if (!isVisibilityHidden('list','code')) {
                        <div [awagVisibility]="visibilityConfig['list']['code']">
                          <a class="link-body-emphasis link-underline link-underline-opacity-0 link-opacity-50-hover awagTableHeader-code" id="list-awagTableHeader-code" href="javascript:void(0)" [awagSortTable]="listListCtrl" sortKey="code">
                            <span>{{ 'awag.prj.uc.maintenance.sc.land.fl.list.code' | translate }}</span>
                            <i class="bi bi-arrow-up"></i><i class="bi bi-arrow-down"></i>
                          </a>
                        </div>
                      }
                      </th>
                    </tr>
                  </thead>
                  <tbody class="table-group-divider">
                  @for (item of model.list | slice:listListCtrl.pager.start:listListCtrl.pager.end; track $any(item) && $any(item).$$awagIndex !== undefined ? $any(item).$$awagIndex : $index; let i = $index) {
                    <tr>
                      <!-- 選択: BUTTON -->
                      <td class="align-top">
                      @if (!isVisibilityHidden('list','select')) {
                        <div class="" [awagVisibility]="visibilityConfig['list']['select']">
                          <button class="btn btn-outline-primary awag-table-button select-{{$any(item).$$awagIndex}}" id="list-select-{{i}}" name="select-{{$any(item).$$awagIndex}}" type="button" (click)="!eventProcessing && select(i, item)" [disabled]="eventProcessing || isVisibilityInactive('list','select')">
                            <span>{{ 'awag.prj.uc.maintenance.sc.land.fl.list.select' | translate }}</span>
                          </button>
                        </div>
                      }
                      </td>
                      <!-- 支局等: OUTPUT -->
                      <td class="align-top">
                      @if (!isVisibilityHidden('list','branch')) {
                        <div class="" [awagVisibility]="visibilityConfig['list']['branch']">
                          <span class="branch-{{$any(item).$$awagIndex}}" id="list-branch-{{i}}">{{ $any(item).branch }}</span>
                        </div>
                      }
                      </td>
                      <!-- 標版文字: OUTPUT -->
                      <td class="align-top">
                      @if (!isVisibilityHidden('list','name')) {
                        <div class="" [awagVisibility]="visibilityConfig['list']['name']">
                          <span class="name-{{$any(item).$$awagIndex}}" id="list-name-{{i}}">{{ $any(item).name }}</span>
                        </div>
                      }
                      </td>
                      <!-- コード: OUTPUT -->
                      <td class="align-top">
                      @if (!isVisibilityHidden('list','code')) {
                        <div class="" [awagVisibility]="visibilityConfig['list']['code']">
                          <span class="code-{{$any(item).$$awagIndex}}" id="list-code-{{i}}">{{ $any(item).code }}</span>
                        </div>
                      }
                      </td>
                    </tr>
                  }
                  </tbody>
                </table>
              </div>
            @if (!isVisibilityHidden('list')) {
              <nav class="d-flex pe-2 awag-pagination" attr.aria-label="{{ 'awag.label.sc.pagination' | translate }}" [awagVisibility]="visibilityConfig['list']['*']">
                <div class="d-flex flex-nowrap mb-3 mt-1 awag-pagination-len">
                  <label class="col-form-labe m-2" for="list-len-select">{{ 'awag.label.sc.pagination.len.select' | translate}}</label>
                  <select style="width: 4.5rem" class="form-select form-select-sm" id="list-paginationSizeSelect" name="paginationSizeSelect" [(ngModel)]="listListCtrl.pager.len" (change)="changelen(listListCtrl, listListCtrl.pager.len)" [disabled]="isVisibilityInactive('list')">
                  @for (opt of listListCtrl.pager.lenList; track $index) {
                    <option [value]="opt">{{opt}}</option>
                  }
                  </select>
                </div>
                <div class="d-flex flex-nowrap mb-3 mt-1 flex-grow-1 justify-content-end awag-pagination-info">
                @if (model.list?.length > 0) {
                  <span class="col-form-label px-2">
                    {{ listListCtrl.pager.start + 1 }} - {{ listListCtrl.pager.end }}
                    {{ 'awag.label.sc.pagination.info.range' | translate }}
                    {{ model.list ? model.list.length : '0' }}
                    {{ 'awag.label.sc.pagination.info.total' | translate }}
                  </span>
                }
                @if (model.list?.length == 0 || !model.list) {
                  <span class="col-form-label px-2">
                    0 {{ 'awag.label.sc.pagination.info.items' | translate }}
                  </span>
                }
                </div>
                <div class="d-flex flex-nowrap mt-1 justify-content-end awag-pagination-nav">
                  <ngb-pagination
                      [(page)]="listListCtrl.pager.pagenum"
                      [pageSize]="listListCtrl.pager.len"
                      [collectionSize]="model.list?.length"
                      [ellipses]="false"
                      [boundaryLinks]="true"
                      [maxSize]="5"
                      [rotate]="true"
                      [disabled]="isVisibilityInactive('list')"
                      (pageChange)="listListCtrl.paginate($event)">
                    <ng-template ngbPaginationFirst><span class="paginationFirst" id="list-paginationFirst">««</span></ng-template>
                    <ng-template ngbPaginationPrevious><span class="paginationPrevious" id="list-paginationPrevious">«</span></ng-template>
                    <ng-template ngbPaginationNumber let-page><span class="paginationPage-{{page}}" id="list-paginationPage-{{page}}">{{ page }}</span></ng-template>
                    <ng-template ngbPaginationNext><span class="paginationNext" id="list-paginationNext">»</span></ng-template>
                    <ng-template ngbPaginationLast><span class="paginationLast" id="list-paginationLast">»»</span></ng-template>
                  </ngb-pagination>
                </div>
              </nav>
            }
              <div class="d-grid gap-2 d-md-flex justify-content-md-center">
              @if (!isVisibilityHidden('list','cancel')) {
                <button class="btn btn-primary" id="list-cancel" name="cancel" type="button" (click)="!eventProcessing && cancel()" [disabled]="eventProcessing || isVisibilityInactive('list', 'cancel')" [awagVisibility]="visibilityConfig['list']['cancel']">
                  <span>{{ 'awag.prj.uc.maintenance.sc.land.fl.list.cancel' | translate }}</span>
                </button>
              }
              </div>
            </ng-template>
          </div>
        </div>
      </div>
    </div>
  </div>
}
</form>
<div class="awag-footer-spacer"></div>
